{
  "name": "20-20-20",
  "description": "Timer app for your eyes",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["alarms", "notifications", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "32": "icons/eye32.png",
    "48": "icons/eye48.png",
    "128": "icons/eye128.png"
  }
}